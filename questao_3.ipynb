   "source": [
    "# Input de N\n",
    "N = int(input(\"Digite o valor de N: \"))\n",
    "\n",
    "# Inicializa a soma\n",
    "S = 0\n",
    "\n",
    "# Loop para calcular a soma\n",
    "for i in range(1, N + 1):\n",
    "    S += i / (N - i + 1)\n",
    "\n",
    "# Mostra o resultado\n",
    "print(f\"O valor de S é: {S}\")\n"
   ]
  }
